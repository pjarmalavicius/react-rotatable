{
  "name": "react-rotatable",
  "version": "1.0.2",
  "description": "React component to add rotate possibility",
  "main": "dist/index.js",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint",
    "open:src": "babel-node tools/srcServer.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint",
    "build": "babel src --out-dir dist && npm run sass",
    "sass": "node-sass src/styles/rotatable.scss dist/css/rotatable.min.css --output-style compressed",
    "lint": "eslint src",
    "prepublish": "npm run lint && npm run test && npm run clean-dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pjarmalavicius/react-rotatable.git"
  },
  "keywords": [
    "react-rotatable",
    "rotatable",
    "react",
    "react-component"
  ],
  "author": "Paulius JarmalaviÄius",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pjarmalavicius/react-rotatable/issues"
  },
  "homepage": "https://pjarmalavicius.github.io/react-rotatable/",
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.0.0",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "browser-sync": "^2.23.2",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.8",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.9.0",
    "expect": "^1.6.0",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^2.2.5",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rimraf": "^2.3.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-dev-server": "^1.8.2",
    "webpack-hot-middleware": "^2.21.0"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "dependencies": {}
}
